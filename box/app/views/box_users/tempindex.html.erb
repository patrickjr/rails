<p id="notice"><%= notice %></p>

<h1>Listing Box Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @box_users.each do |box_user| %>
      <tr>
        <td><%= link_to 'Show', box_user %></td>
        <td><%= link_to 'Edit', edit_box_user_path(box_user) %></td>
        <td><%= link_to 'Destroy', box_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Box user', new_box_user_path %>
